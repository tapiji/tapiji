<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="Tapiji Translator">
         <property
               name="windowImages"
               value="platform:/plugin/org.eclipse.e4.tapiji.resource/icons/16x16/tapiji.png,platform:/plugin/org.eclipse.e4.tapiji.resource/icons/32x32/tapiji.png,platform:/plugin/org.eclipse.e4.tapiji.resource/icons/48x48/tapiji.png,platform:/plugin/org.eclipse.e4.tapiji.resource/icons/64x64/tapiji.png,platform:/plugin/org.eclipse.e4.tapiji.resource/icons/128x128/tapiji.png">
         </property>
         <property
               name="aboutText"
               value="TapiJI - Translator 
Version 1.0.0
by Stefan Strobl &amp;amp;
Martin Reiterer &amp;amp;
Christian Behon">
         </property>
         <property
               name="appName"
               value="Tapiji Translator">
         </property>
         <property
               name="lifeCycleURI"
               value="bundleclass://org.eclipse.e4.tapiji.glossary/org.eclipse.e4.tapiji.glossary.LifeCycleManager">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/org.eclipse.e4.tapiji.glossary/css/default.css">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.eclipse.e4.tapiji.glossary.definition.translationreference">
         <with
               variable="org.eclipse.e4.tapiji.glossary.popupmenu.TRANSLATION_REFERENCE">
            <and>
               <not>
                  <equals
                        value="0">
                  </equals>
               </not>
            </and>
         </with>
      </definition>
      <definition
            id="org.eclipse.e4.tapiji.glossary.definition.translationvisibility">
         <with
               variable="org.eclipse.e4.tapiji.glossary.popupmenu.TRANSLATION_VISIBILITY">
            <and>
               <not>
                  <equals
                        value="0">
                  </equals>
               </not>
            </and>
         </with>
      </definition>
   </extension>
   <extension
         id="org.eclipse.e4.tapiji.glossary.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>

</plugin>
